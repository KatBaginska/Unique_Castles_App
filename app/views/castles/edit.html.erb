<div class="container mt-5">
<h1>Edit castle listing</h1>
<%= simple_form_for(@castle) do |f| %>
  <%= f.input :title %>
  <%= f.input :price %>
  <%= f.input :description %>
  <%= f.input :location %>
  <%= f.label "Castle Photo" %>
  <%= f.file_field :avatar %><br>
  <%= f.submit "Update" %>
<% end %>
</div>
