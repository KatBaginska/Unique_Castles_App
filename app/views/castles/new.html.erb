<div class="container mt-5">
<h1>Create castle listing</h1>
<%= simple_form_for(@castle) do |f| %>
  <%= f.input :title %>
  <%= f.input :price %>
  <%= f.input :description %>
  <%= f.input :location %>
  <%= f.input :photos, as: :file, input_html: { multiple: true } %>
  <%= f.submit "Create" %>
<% end %>
</div>
